{
  "compilerOptions": {
    "target": "esnext",
    "module": "es2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "alwaysStrict": true,
    "strict": true,
    "noEmit": true,
    "noImplicitOverride": true,
    "removeComments": true,
    "declaration": false,
    "skipLibCheck": true,
    "newLine": "crlf"
  }
}
